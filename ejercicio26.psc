Algoritmo ejercicio26
	Definir menorAMayor,mayorAMenor Como Entero
	menorAMayor <- 0
	Mientras menorAMayor<=100 Hacer
		Escribir menorAMayor
		menorAMayor <- menorAMayor+1
	FinMientras
	mayorAMenor <- menorAMayor -1
	Mientras mayorAMenor>=1 Hacer
		Escribir mayorAMenor
		mayorAMenor <- mayorAMenor-1
	FinMientras
	
FinAlgoritmo
