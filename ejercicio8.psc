Algoritmo ejercicio8
	definir n1,n2,suma,resta,mult,divis como Real
	Escribir "Ingrese un numero entero"
	leer n1
	Escribir "Ingrese otro numero entero"
	leer n2
	suma <- n1+n2
	resta <- n1-n2
	mult <- n1*n2
	divis <- n1/n2
	Escribir "La suma entre ",n1 " y ",n2 " es = ",suma ", la resta es = ", resta ", la multiplicación es = ", mult " y la division es = ", divis
FinAlgoritmo
